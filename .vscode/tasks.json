{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-current",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${fileDirname}",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile",
            "options": {
                "env": {
                    "DOTNET_ROOT": "${env:HOME}/.dotnet"
                }
            }
        }
    ]
}
